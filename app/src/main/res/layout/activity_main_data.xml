<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainData">

    <TextView
        android:id="@+id/textView"
        android:layout_width="184dp"
        android:layout_height="42dp"
        android:layout_marginTop="8dp"
        android:text="AvailableMoney"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/MainMoney"
        app:layout_constraintHorizontal_bias="0.761"
        app:layout_constraintStart_toStartOf="@+id/MainMoney"
        app:layout_constraintTop_toBottomOf="@+id/MainMoney" />

    <TextView
        android:id="@+id/MainMoney"
        android:layout_width="251dp"
        android:layout_height="49dp"
        android:layout_marginTop="36dp"
        android:text="7000 Rs"
        android:textAlignment="center"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/MainAddexpenses"
        android:layout_width="162dp"
        android:layout_height="50dp"
        android:layout_marginTop="176dp"
        android:text="Add Expenses"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.843"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/MainAlltransiction"
        android:layout_width="141dp"
        android:layout_height="63dp"
        android:text="All Transictions"
        app:layout_constraintBottom_toBottomOf="@+id/MainAddexpenses"
        app:layout_constraintEnd_toStartOf="@+id/MainAddexpenses"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/MainAddexpenses" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="173dp"
        android:layout_height="30dp"
        android:layout_marginTop="16dp"
        android:text="Members"
        android:textColor="@color/common_google_signin_btn_text_dark_focused"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/MainAddexpenses"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.067"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MainAlltransiction"
        app:layout_constraintVertical_bias="0.018" />

    <ListView
        android:id="@+id/List"
        android:layout_width="407dp"
        android:layout_height="417dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/Logout"
        android:layout_width="133dp"
        android:layout_height="37dp"
        android:layout_marginTop="12dp"
        android:text="Logout"
        app:layout_constraintEnd_toEndOf="@+id/MainAddexpenses"
        app:layout_constraintHorizontal_bias="0.62"
        app:layout_constraintStart_toStartOf="@+id/MainAddexpenses"
        app:layout_constraintTop_toBottomOf="@+id/MainAddexpenses" />

</androidx.constraintlayout.widget.ConstraintLayout>